package vine.settings;

import java.io.FileInputStream;
import java.util.Collection;

/**
 * @author Steffen
 *
 */
public class SettingsCollection {
    /**
     * 
     */
    Collection<Setting> settingFields = null;

    /**
     * 
     */
    public SettingsCollection() {
    }

    /**
     * @param settings
     */
    public void addSettings(Collection<Setting> settings) {
        if (this.settingFields == null) {
            this.settingFields = settings;
        } else {
            this.settingFields.addAll(settings);
        }
    }

    public void load(FileInputStream reader) {

    }

}
