package vine.event;

public abstract class Event {
    enum Type {
        MOUSE_PRESSED,
        MOUSE_RELEASED,
        MOUSE_MOVED,
        KEY_PRESSED,
        KEY_HOLD,
        KEY_RELEASED
    }
    
    public abstract Type getType();

    public void setHandled(boolean onEvent) {
        
    }

    public boolean isHandled() {
        // TODO Auto-generated method stub
        return false;
    }
}
