package vine.gameplay.scene;

/**
 * @author Steffen
 *
 */
class SceneCreationException extends Exception {
    /**
     * 
     */
    private static final long serialVersionUID = -3339224831584061401L;

    /**
     * @param message
     *            The error message.
     */
    protected SceneCreationException(String message) {
        super(message);
    }
}
